<p id="notice"><%= notice %></p>

<h1>Times Cadastrados</h1>
<fieldset>
<legend>Meus Times</legend>
<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th>Descricao</th>
      <th>Casa</th>
      <th>Mascote</th>
      <th>Rival</th>
      <th>Tecnico</th>
      <th>Cidade</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  </fieldset>

  <tbody>
    <% @cadastros.each do |cadastro| %>
      <tr>
        <td><%= cadastro.Nome %></td>
        <td><%= cadastro.Descricao %></td>
        <td><%= cadastro.Casa %></td>
        <td><%= cadastro.Mascote %></td>
        <td><%= cadastro.Rival %></td>
        <td><%= cadastro.Tecnico %></td>
        <td><%= cadastro.Cidade %></td>
        <td><%= cadastro.Estado %></td>
        <td><%= link_to 'Exibir', cadastro %></td>
        <td><%= link_to 'Editar', edit_cadastro_path(cadastro) %></td>
        <td><%= link_to 'Excluir', cadastro, method: :delete, data: { confirm: 'Deseja excluir?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
<%= link_to 'Novo Time', new_cadastro_path %>
